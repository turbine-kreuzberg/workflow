{
  "name": "turbine/workflow",
  "description": "",
  "license": "MIT",
  "require": {
    "php": ">=7.4.3"
  },
  "require-dev": {
    "codeception/c3": "2.6.1",
    "codeception/codeception": "4.1.12",
    "codeception/module-asserts": "1.3.1",
    "codeception/module-cli": "1.0.4",
    "codeception/module-filesystem": "1.0.3",
    "codeception/module-phpbrowser": "1.0.2",
    "codeception/module-rest": "1.2.7",
    "codeception/module-webdriver": "1.1.4",
    "symfony/console": "^5.1"
  },
  "autoload": {
  },
  "autoload-dev": {
    "psr-4": {
      "Workflow\\": "workflow"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "platform": {
      "php": "7.4.3"
    },
    "use-include-path": true,
    "sort-packages": true,
    "github-protocols": ["https"],
    "process-timeout": 900
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://git.votum-media.net/turbine/workflow.git"
    }
  ]
}
